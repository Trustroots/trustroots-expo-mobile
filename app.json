{
  "expo": {
    "name": "Trustroots",
    "version": "1.0.0",
    "description": "Travellers community for sharing, hosting and getting people together.",
    "slug": "trustroots",
    "scheme": "trustroots",
    "privacy": "public",
    "sdkVersion": "22.0.0",
    "orientation": "portrait",
    "primaryColor": "#12b591",
    "icon": "./assets/icon_app.png",
    "loading": {
      "icon": "./assets/loading.png",
      "backgroundColor": "#12b591",
      "hideExponentText": true
    },
    "splash": {
      "backgroundColor": "#12b591",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "notification": {
      "icon": "./assets/icon_push.png",
      "color": "#12b591",
      "androidMode": "default"
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "android": {
      "package": "org.trustroots.trustrootsApp",
      "icon": "./assets/icon_app.png",
      "permissions": []
    },
    "ios": {
      "bundleIdentifier": "org.trustroots.trustrootsApp",
      "supportsTablet": true,
      "icon": "./assets/icon_app.png"
    }
  }
}
