{
  "expo": {
    "version": "1.2.0",
    "name": "Trustroots",
    "slug": "trustroots",
    "scheme": "trustroots",
    "description": "Travellers community for sharing, hosting and getting people together.",
    "githubUrl": "https://github.com/Trustroots/trustroots-expo-mobile",
    "privacy": "public",
    "sdkVersion": "37.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "primaryColor": "#12b591",
    "icon": "assets/icon-1024.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "loading": {
      "icon": "assets/loading.png",
      "backgroundColor": "#12b591",
      "hideExponentText": true
    },
    "splash": {
      "backgroundColor": "#12b591",
      "image": "assets/splash.png",
      "resizeMode": "contain"
    },
    "notification": {
      "icon": "assets/icon_push.png",
      "color": "#12b591",
      "androidMode": "default"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "android": {
      "versionCode": 3,
      "package": "org.trustroots.trustrootsApp",
      "icon": "assets/icon-512.png",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=org.trustroots.trustrootsApp"
    },
    "ios": {
      "bundleIdentifier": "org.trustroots.trustrootsApp",
      "supportsTablet": true,
      "icon": "assets/icon-1024.png"
    }
  }
}
